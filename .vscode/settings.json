{
    "cSpell.words": [
        "Codimension",
        "Hennion",
        "Nussbaum",
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",        
                "-outdir=%OUTDIR%",
                "-output-directory=build",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.outDir": "%DIR%/build"
}